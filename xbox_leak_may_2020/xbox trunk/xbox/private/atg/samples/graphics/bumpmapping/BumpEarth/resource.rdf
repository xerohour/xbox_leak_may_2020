// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         Resource.h
out_packedresource Media\Resource.xpr
out_error          Resource.err
out_prefix         resource

Texture Block
{
   Source      Media\Textures\Block.bmp
}

Texture Earth
{
   Source      Media\Textures\Earth.bmp
   Format      D3DFMT_X8R8G8B8
   Levels      0
}

Texture EarthBump
{
   Source      Media\Textures\EarthBump.bmp
   Format      D3DFMT_X8R8G8B8
   Levels      0
}

Texture EarthEnvMap
{
   Source      Media\Textures\EarthEnvMap.bmp
   Format      D3DFMT_X8R8G8B8
   Levels      0
}

