// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         Font12.h
out_packedresource Media\Font12.xpr
out_error          Font12.err


Texture Font
{
   Source      Media\Fonts\Arial_12.tga
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile Media\Fonts\Arial_12.abc
}

