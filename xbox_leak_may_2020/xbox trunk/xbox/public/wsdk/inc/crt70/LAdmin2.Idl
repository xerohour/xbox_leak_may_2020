		[propget, helpstring("method ReadOnlyMode")] HRESULT ReadOnlyMode([out, retval] VARIANT *pData);
		[propput, helpstring("method ReadOnlyMode")] HRESULT ReadOnlyMode([in] VARIANT Data);

